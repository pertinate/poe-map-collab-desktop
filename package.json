{
    "name": "poe-map-collab",
    "version": "1.3.0",
    "repository": "https://github.com/awohletz/electron-prisma-trpc-example-releases",
    "description": "An example repo showing how to use Electron with tRPC and Prisma",
    "main": "dist/server/main.js",
    "watch": {
        "dev-server": {
            "patterns": [
                "src/server"
            ],
            "extension": "ts"
        }
    },
    "scripts": {
        "build": "vite build && prisma generate && tsc --build && node copy-files.js && node install-engines-on-mac.js",
        "electron-build": "tsc --build && node copy-files.js && node install-engines-on-mac.js",
        "start": "npm run build && concurrently \"vite\" \"npm-watch dev-server\"",
        "dev-client": "vite",
        "dev-server-watch": "npm-watch \"dev-server\"",
        "dev-server": "npm run electron-build && cross-env NODE_ENV=development ELECTRON_ENABLE_LOGGING=1 electron .",
        "pack": "npm run build && electron-builder --dir",
        "dist": "npm run build && electron-builder",
        "publish": "npm run build && dotenv -- electron-builder -p always"
    },
    "author": "Ayron Wohletz",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^18.11.17",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@vitejs/plugin-react": "^3.0.0",
        "autoprefixer": "^10.4.19",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "dotenv-cli": "^6.0.0",
        "electron": "^22.0.0",
        "electron-builder": "^23.6.0",
        "electron-notarize": "^1.2.1",
        "electron-reloader": "^1.2.3",
        "fs-extra": "^11.1.0",
        "nodemon": "^3.1.0",
        "npm-watch": "^0.11.0",
        "postcss": "^8.4.38",
        "replace-in-file": "^6.3.5",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.3",
        "vite": "^4.0.2"
    },
    "dependencies": {
        "@mantine/carousel": "^7.7.0",
        "@mantine/charts": "^7.7.0",
        "@mantine/code-highlight": "^7.7.0",
        "@mantine/core": "^7.7.0",
        "@mantine/dates": "^7.7.0",
        "@mantine/dropzone": "^7.7.0",
        "@mantine/form": "^7.7.0",
        "@mantine/hooks": "^7.7.0",
        "@mantine/modals": "^7.7.0",
        "@mantine/notifications": "^7.7.0",
        "@mantine/nprogress": "^7.7.0",
        "@mantine/spotlight": "^7.7.0",
        "@mantine/tiptap": "^7.7.0",
        "@prisma/client": "^4.7.1",
        "@prisma/engines": "^4.7.1",
        "@tabler/icons-react": "^3.1.0",
        "@tanstack/react-query": "^4.20.4",
        "@tanstack/react-router": "^1.23.1",
        "@tanstack/router-devtools": "^1.23.1",
        "@tiptap/extension-link": "^2.2.4",
        "@tiptap/react": "^2.2.4",
        "@tiptap/starter-kit": "^2.2.4",
        "@trpc/client": "^10.5.0",
        "@trpc/react-query": "^10.5.0",
        "@trpc/server": "^10.5.0",
        "auto-prefixer": "^0.4.2",
        "chokidar": "^3.6.0",
        "dayjs": "^1.11.10",
        "electron-log": "^4.4.8",
        "embla-carousel-react": "^8.0.0",
        "postcss-import": "^16.1.0",
        "postcss-preset-mantine": "^1.13.0",
        "postcss-simple-vars": "^7.0.1",
        "prisma": "^4.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.2",
        "recharts": "^2.12.3",
        "redis": "^4.6.13",
        "say": "^0.16.0",
        "superjson": "^1.12.1",
        "zod": "^3.20.2"
    }
}